          <description>
            0 = Code protection off
            1 = 0000h to 07FFh code protected
          </description>
        </deviceBit>
        <deviceBit bit="0" name="Data Code Protection" address="0x400f" inverse="true" code="CPD" >
          <description>
            0 = Data memory code protection off
